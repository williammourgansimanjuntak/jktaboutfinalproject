<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/backgroundnamamember"
    tools:context=".Namanamamember3Activity">

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        android:text="NAMA MEMBER"
        android:textSize="30dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <Button
        android:id="@+id/back1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="28dp"
        android:layout_marginBottom="30dp"
        android:background="@color/white"
        android:text="BACK"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageButton
        android:id="@+id/btnfritzy"
        android:layout_width="136dp"
        android:layout_height="184dp"
        android:layout_marginStart="29dp"
        android:layout_marginTop="121dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/fritzyjkt"
        tools:layout_editor_absoluteX="35dp"
        tools:layout_editor_absoluteY="108dp" />

    <ImageButton
        android:id="@+id/btnadel"
        android:layout_width="136dp"
        android:layout_height="184dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="260dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@id/btnfritzy"
        app:srcCompat="@drawable/adeljkt" />


    <TextView
        android:id="@+id/tentangFritzy1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="64dp"
        android:text="NAMA         : Fritzy Rosmerian"
        android:textSize="15dp"
        app:layout_constraintStart_toEndOf="@+id/btnfritzy"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <TextView
        android:id="@+id/tentangFritzy2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:text="TGL LAHIR : 28 juli 2008"
        android:textSize="15dp"
        app:layout_constraintStart_toEndOf="@id/btnfritzy"
        app:layout_constraintTop_toBottomOf="@id/tentangFritzy1" />

    <TextView
        android:id="@+id/tentangFritzy3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:text="HOBI           : Sulap"
        android:textSize="15dp"
        app:layout_constraintStart_toEndOf="@+id/btnfritzy"
        app:layout_constraintTop_toBottomOf="@id/tentangFritzy2" />


    <TextView
        android:id="@+id/tentangFritzy4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:text="GENERASI  : Gen 12"
        android:textSize="15dp"
        app:layout_constraintStart_toEndOf="@+id/btnfritzy"
        app:layout_constraintTop_toBottomOf="@id/tentangFritzy3" />


    <TextView
        android:id="@+id/tentangAdel1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="180dp"
        android:text="NAMA         : Reva Fidela "
        android:textSize="15dp"
        app:layout_constraintStart_toEndOf="@+id/btnadel"
        app:layout_constraintTop_toBottomOf="@+id/tentangFritzy4" />

    <TextView
        android:id="@+id/tentangAdel2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:text="TGL LAHIR : 14 Juli 2006"
        android:textSize="15dp"
        app:layout_constraintStart_toEndOf="@+id/btnadel"
        app:layout_constraintTop_toBottomOf="@id/tentangAdel1" />

    <TextView
        android:id="@+id/tentangAdel3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:text="HOBI           : Mengambar "
        android:textSize="15dp"
        app:layout_constraintStart_toEndOf="@+id/btnadel"
        app:layout_constraintTop_toBottomOf="@id/tentangAdel2" />

    <TextView
        android:id="@+id/tentangAdel4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:text="GENERASI  : Gen 8"
        android:textSize="15dp"
        app:layout_constraintStart_toEndOf="@+id/btnadel"
        app:layout_constraintTop_toBottomOf="@id/tentangAdel3" />

    <ImageButton
        android:id="@+id/homepagebtn"
        android:layout_width="148dp"
        android:layout_height="43dp"
        android:layout_marginStart="93dp"
        android:layout_marginBottom="30dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/back1"
        app:srcCompat="@drawable/logohomepage" />


</androidx.constraintlayout.widget.ConstraintLayout>